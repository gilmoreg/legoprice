{
  "name": "legoprice",
  "version": "1.0.0",
  "description": "Quickly compare current prices for Lego sets by set number",
  "main": "index.js",
  "scripts": {
    "start": "node server/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
